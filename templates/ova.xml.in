<?xml version="1.0" ?>
    <appliance version="0.1">
        <vm name="vm">
            <label>@TITLE@</label>
            <shortdesc>@DESCRIPTION@</shortdesc>
            <config mem_set="@MEMORY@" vcpus="1"/>
            <hacks is_hvm="false" kernel_boot_cmdline="root=/dev/sda1 ro ">
            </hacks>
@VDB_ENTRIES@
        </vm>
@VDI_ENTRIES@
</appliance>
